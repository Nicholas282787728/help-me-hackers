<h1>New vote</h1>

<% form_for(@vote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :vote %><br />
    <%= f.check_box :vote %>
  </p>
  <p>
    <%= f.label :voteable_id %><br />
    <%= f.text_field :voteable_id %>
  </p>
  <p>
    <%= f.label :voteable_type %><br />
    <%= f.text_field :voteable_type %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', votes_path %>